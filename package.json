{
  "name": "react-native-app",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise React Native Application",
  "author": "Enterprise Team <dev@company.com>",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/company/react-native-app.git"
  },
  "keywords": [
    "react-native",
    "enterprise",
    "mobile",
    "typescript"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "doctor": "react-native doctor",
    "clean": "rm -rf node_modules && cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "clean:install": "npm run clean && npm install",
    "postinstall": "patch-package && react-native-asset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:verbose": "jest --verbose",
    "test:update": "jest --updateSnapshot",
    "test:clear": "jest --clearCache",
    "test:profile": "jest --logHeapUsage",
    "test:e2e": "detox test --configuration ios.sim.release",
    "test:performance": "react-native-performance --platform ios",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:build": "tsc -p tsconfig.build.json --noEmit",
    "type-check:test": "tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "type-check": "tsc --noEmit",
    "prepush": "npm run lint && npm run type-check && npm run test",
    "build:android:dev": "cd android && ./gradlew assembleDebug",
    "build:android:prod": "cd android && ./gradlew assembleRelease",
    "build:ios:dev": "cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration Debug",
    "build:ios:prod": "cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release",
    "build:optimized": "cross-env NODE_ENV=production npm run bundle:android",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:analyze": "react-native-bundle-visualizer",
    "release:staging": "react-native run-android --variant=staging",
    "release:production": "react-native run-android --variant=release",
    "release": "standard-version && git push --follow-tags origin main",
    "deploy:staging": "appcenter codepush release-react -a company/app-android -d Staging",
    "deploy:production": "appcenter codepush release-react -a company/app-android -d Production",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "security:scan": "audit-ci --moderate --pass-enoaudit",
    "security:licenses": "license-checker --production --json",
    "storybook": "start-storybook -p 7007",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
  
  },
  "devDependencies": {
   
  }
}

